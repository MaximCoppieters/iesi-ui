{
  "name": "iesi-ui",
  "version": "0.1.0",
  "description": "Visual user interface for the IESI automation framework.",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "npm run lint && craco test --transformIgnorePatterns \"node_modules/(?!(@snipsonian))\"",
    "eslint": "eslint --ext .js,.ts,.tsx src bin *.js",
    "stylelint": "stylelint src/**/*.scss",
    "lint": "npm run eslint && npm run stylelint",
    "init-env-config": "node bin/init/initEnvConfigFile.js"
  },
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@reach/router": "^1.3.3",
    "@snipsonian/axios": "^1.11.1",
    "@snipsonian/browser": "^1.11.0",
    "@snipsonian/core": "^1.11.0",
    "@snipsonian/dvlp": "^1.11.0",
    "@snipsonian/observable-state": "^1.11.0",
    "@snipsonian/react-observable-state": "^1.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "stylelint-webpack-plugin": "^1.2.3",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@kunstmaan/eslint-typescript-config": "^2.0.1",
    "@kunstmaan/stylelint-config": "^1.2.0",
    "@snipsonian/node": "^1.8.0",
    "@types/classnames": "^2.2.10",
    "@types/reach__router": "^1.3.2",
    "chalk": "^4.0.0",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "http-proxy-middleware": "^1.0.3",
    "winston": "^3.2.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
